/*8. Write a function in C to print all unique elements in an array.*/

#include <stdio.h>

int unique(int a[]);
int n=5, count=0;
int main()
{   int i,b[5]={1,3,4,0,5};
    printf("duplicate = %d",unique(b));

    return 0;
}

int unique(int a[])

{
    
   
   
    for(int i=0; i<n; i++)
    
     {  int x=i;
         
         for(int j=i+1; j<n;j++){
            if(a[i]==a[j]) // if 2 elements aresame then dont print that element(a[i] )and check for the next one(a[i+1]) 
            {
               i++;
            }
            
           
         } 
         if (x==i);
         printf("%d, ",a[i]); 
     }
    
     
     return count;
    
//print

    for(int i=0; i<n;i++)
     {
         printf("%d ",a[i]);
     }
}
