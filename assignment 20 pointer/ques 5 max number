//5. Write a program to find the maximum number between two numbers using a pointer


#include <stdio.h>
void max(int *p1, int *p2);
int main()
{   int num1, num2;
    
    printf("enter two numbers");
    scanf("%d",& num1);
     scanf("%d",& num2);
   max(&num1, &num2);

    return 0;
}

void max(int *p1, int *p2)
{
    if(*p1>*p2)
        printf("%d is max",*p1);
        
         else if(*p1<*p2)
        printf("%d is max",*p2);
        
         else
        printf("both are equal");
        
}
