//9. Write a program to print the elements of an array in reverse order.


#include <stdio.h>
void rev(int *p);
int main()
{   int arr[10]={1,5,5,6,7,2,1,7,9,3};
    
  
     
   rev(arr); 
    return 0;
}

void rev(int *p)
{
   int sum=0;
   for(int i=9; i>=0; i--) 
   {
       printf("%d ",p[i]);
   }
   
  
        
}
