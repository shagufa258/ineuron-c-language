#include <stdio.h>
void num(int n);
int main()
{int i,j;
    for(i=1;i<=6;i++)
    {
        for(j=1;j<=9;j++)
        {
            if(j>=i && j<=10-i)
            {
                printf("*");
                
            }
            else
                printf(" ");
        }
        printf("\n");
    }
return 0;
}
