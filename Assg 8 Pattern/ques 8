print the pattern-
*********
 *******
  *****
   *** 
    *


#include <stdio.h>
void num(int n);
int main()
{int i,j, num=7;
    for(i=1;i<=num;i++)
    {
        for(j=1;j<=num*2-1;j++)
        {
            if(j>=i && j<=num*2-i)
            {
                printf("*");
                
            }
            else
                printf(" ");
        }
        printf("\n");
    }
return 0;
}
