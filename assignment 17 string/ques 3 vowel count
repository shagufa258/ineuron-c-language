//3. Write a program to count vowels in a given string

#include <stdio.h>
void vowel_count(char a[]);
int main()
    
{   
    char x[10]={'s','h','a','i','u','f','a'};
    vowel_count(x);
    return 0;
}

void vowel_count(char a[])
{
    int count=0;

    for(int i=0; i<10; i++)
    {   if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')
            count+=1;
     
    }
    
    printf("number of vowel= %d ",count);
    
    
    
}

