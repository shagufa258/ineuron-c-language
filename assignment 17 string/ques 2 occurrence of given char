//2. Write a program to count the occurrence of a given character in a given string.


#include <stdio.h>
void size(char a[], char character);
int main()
    
{   
    char x[10]={'s','h','a','g','u','f','a'};
    size(x, 's');
    return 0;
}

void size(char a[], char character)
{
    int count=0;

    for(int i=0; i<10; i++)
    {   if(a[i]==character)
            count+=1;
     
    }
    
    printf(" %c comes %d time(s)", character,count);
    
    
    
}

