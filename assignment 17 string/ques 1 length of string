/*1. Write a program to calculate the length of the string. (without using built-in method)*/

#include <stdio.h>
void size(char a[]);
int main()
    
{   
    char x[10]={'s','h','a','g','u','f','a'};
    size(x);
    return 0;
}

void size(char a[])
{
    int count=0;

    for(int i=0; i<10; i++)
    {   if(a[i]!=0)
            count+=1;
        else
        break;
      
    }
    
    printf("length= %d", count);
    
    
    
}

