#include <stdio.h>

int main()
{int i, j, largest, s_largest, n, a[100];
  printf("size of array- ");
  scanf("%d",&n);
  for(i=0; i<n; i++)
  {
    scanf("%d",&a[i]);
    
  }
  //finding 2nd largest
  largest=a[0];
  for(i=1;i<n;i++)
  {
      
      
      if(a[i]>largest)
          {  
          s_largest=largest;
          largest=a[i];}
      
      else
        if(a[i]>s_largest)
        {
            s_largest=a[i];
        }
   
  }
  
  
   printf("\nl= %d, ",largest);
    printf("\nSL= %d, ",s_largest);
  
  
