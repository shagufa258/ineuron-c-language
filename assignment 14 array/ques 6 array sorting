/*6. Write a program to sort elements of an array of size 10. Take array values from the
user.*/

#include <stdio.h>

int i,j, n=4,a[100]={1,0,3,0,5,2,5,8,6,7}, temp;
void array_sort(int b[]);

int main()
{
    
    array_sort(a);

     return 0;
}

 
 void array_sort(int b[])
 
{
    for(i=0; i<n-1; i++)
    {
        for(j=i+1; j<n;j++)
        {
           if( b[i]>b[j])
           {
             temp=b[i];
             b[i]=b[j];
             b[j]=temp;
             
           }
        }
    }
    for(i=0; i<n;i++)
    {
    printf("%d, ",b[i]);
    }
}

     
     
     
