/**7. Write a program in C to print or display the upper triangular of a given matrix.*/

#include <stdio.h>
void upper_tri(int a[3][3]) ;
int main()
{   int x[3][3]={1,5,3, 4,2,1, 2,5,2};
    upper_tri(x);

    return 0;
}

void upper_tri(int a[3][3])
{   
   for(int i=0; i<3;i++)
     {
    for(int j=0; j<3;j++) 
      {
       if(i>j)
       {
           a[i][j]=0;
       }
       printf("%d ", a[i][j]);
      }
      
  printf("\n");
    }
      
    }
