/*3. Write a function to check whether a given number is Prime or not. (TSRS)*/

#include <stdio.h>
int prime(int n);
int x, min;

int main()
{
    prime(23);

    return 0;
}

int prime(int n)

{   if(n==1 ||n==2)
        printf("%d is prime number",n);
    if(n==0)
          printf("%d is not a prime number",n);
        
        
    for(int i=2; i<n;i++)
    {
        if (n%i==0)
           { printf("%d is not a prime number",n);
            break;}
        else if(i==n-1)
        
            printf("%d is prime number",n);     
       
    }
        
               
}
