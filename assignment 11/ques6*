/*Write a function to print all Prime numbers between two given numbers. (TSRN)*/


#include <stdio.h>
int prime(int n);
int next_prime(int num);
int prime_in_range(int number1, int number2);
int x;
int main()
{
  //next_prime(12);
 // printf("%d",prime(14
 prime_in_range(1,6);

    return 0;
}

int prime(int n)

{   if(n==1 ||n==2)
        return 1;
    if(n==0)
          return 0;
        
        
    for(int i=2; i<n;i++)
    {
        if (n%i==0)
           return 0;
           
        else if(i==n-1)
        
            return 1;     
       
    }
        
               
}
int next_prime(int num)
{
x=num+1;

    while(1)
        {
            
           
            if(prime(x))
                { return x;}
            else
                x++;
        }
}

int prime_in_range(int number1, int number2)
    {   int var=number1;
    
    while(var!=number2)//function skipping the number 2 and goes to next prime num
        {
            printf("%d\t",next_prime(var));
            var+=next_prime(var)-var;
        }
    }
