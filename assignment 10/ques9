/*9. Write a function to check whether a given number contains a given digit or not.
(TSRS)*/


#include <stdio.h>
int check(int num ,int digit);
int rem;
int main()
{  
    printf("check= %d",check(000001, 1));
   

    return 0;
}

int check(int num, int digit)
{  while(num!=0)
    {
        rem=num%10;
        num=num/10;
        
        if(rem==digit)
           return 1;
          
        
    }
return 0;


} 
